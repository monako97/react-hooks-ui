  },
  "scripts": {
    "build": "webpack --mode production",
    "compile": "npm run compile:tsc && npm run compile:types && npm run compile:lessc && npm run compile:postcss",
    "compile:lessc": "node ./compile.js --dir=./src --copy --entry=./src/libs --output=./lib/libs",
    "compile:postcss": "node ./compile.js --dir=./lib",
    "compile:copy": "node ./compile.js --dir=./lib --copy --entry=./src/static --output=./lib/static",
    "compile:tsc": "tsc --removeComments",
    "compile:types": "tsc --declaration --emitDeclarationOnly --outDir types",
    "eslint": "eslint --fix --ext src/**/*.{ts,tsx}",
    "lint": "npm run eslint && npm run stylelint:fix",
    "postcompile": "npm run terser && npm run compile:copy",
    "ppostinstall": "husky install && husky add .husky/pre-commit 'npm run lint'",
    "prebuild": "npm run lint && rm -rf ./example/dist",
    "precompile": "rm -rf ./lib && rm -rf ./types",
    "prepublishOnly": "npm run build && npm run compile && npm version patch",
    "start": "webpack serve --mode development",
    "stylelint": "stylelint src/**/*.{less,css}",
    "stylelint:fix": "stylelint src/**/*.{less,css} --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "terser": "node ./compile.js --dir=./lib --terser"
  },
  "typings": "types/index.d.ts",
  "version": "1.0.28",
  "files": [
    "example/dist",
    "lib",
    "types",
    "README.md",
    "LICENSE"
  ]
}
