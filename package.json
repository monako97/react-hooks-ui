  "scripts": {
    "build": "webpack --mode production",
    "compile": "npm run compile:tsc && npm run compile:types && npm run compile:lessc && npm run compile:postcss",
    "compile:lessc": "node ./compile.js --dir=./src",
    "compile:postcss": "node ./compile.js --dir=./lib",
    "compile:tsc": "tsc --removeComments",
    "compile:types": "tsc --declaration --emitDeclarationOnly --outDir types",
    "eslint": "eslint --fix --ext src/**/*.{ts,tsx}",
    "lint": "npm run eslint && npm run stylelint:fix",
    "postcompile": "babel src/static --out-dir lib/static --copy-files && npm run uglifyjs",
    "prebuild": "npm run lint && rm -rf ./example/dist",
    "precompile": "rm -rf ./lib && rm -rf ./types",
    "prepublish": "npm run build && npm run compile",
    "start": "webpack-dev-server --open --mode development",
    "stylelint": "stylelint src/**/*.{less,css}",
    "stylelint:fix": "stylelint src/**/*.{less,css} --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "uglifyjs": "node ./compile.js --dir=./lib --uglify",
    "precommit": "npm run lint"
  },
  "typings": "types/index.d.ts",
